[
  {
    "image": "${image}",
    "name": "${name}",

    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "${log_group}",
        "awslogs-region": "${region}",
        "awslogs-stream-prefix": "${log_stream_prefix}"
      }
    },

    "portMappings": [
      {
        "hostPort": 0,
        "protocol": "tcp",
        "containerPort": 3000
      }
    ],
    "command": [${command}"],
    "cpu": 0,
    "mountPoints": [],
    "memory": 400,
    "memoryReservation": 400,
    "volumesFrom": [],
    "essential": true,
    "links": []
  }
]
